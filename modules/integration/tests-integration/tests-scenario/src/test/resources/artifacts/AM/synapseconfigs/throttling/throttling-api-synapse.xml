<?xml version="1.0" encoding="UTF-8"?><definitions xmlns="http://ws.apache.org/ns/synapse">
    <sequence name="fault">
        <log level="full">
            <property name="MESSAGE" value="Executing default &quot;fault&quot; sequence"/>
            <property name="ERROR_CODE" expression="get-property('ERROR_CODE')"/>
            <property name="ERROR_MESSAGE" expression="get-property('ERROR_MESSAGE')"/>
        </log>
        <drop/>
    </sequence>
    <sequence name="main">
        <log/>
        <drop/>
    </sequence>
    <api name="stockquote" context="/stockquote">
        <resource methods="GET POST" uri-template="/test">
            <inSequence>
                <header name="To" action="remove"/>
                <property name="RESPONSE" value="true"/>
                <property name="NO_ENTITY_BODY" scope="axis2" action="remove"/>
                <payloadFactory>
                    <format>
                        <ns:getQuoteResponse xmlns:ns="http://services.samples">
                            <ns:return xmlns:ax21="http://services.samples/xsd">
                                <ax21:change>4.212070096051944</ax21:change>
                                <ax21:earnings>-9.567415587431361</ax21:earnings>
                                <ax21:high>-148.1740146577308</ax21:high>
                                <ax21:symbol>IBM</ax21:symbol>
                            </ns:return>
                        </ns:getQuoteResponse>
                    </format>
                </payloadFactory>
                <send/>
            </inSequence>
            <outSequence>
                <property name="Content-Type" value="apllication/xml" scope="transport"/>
                <send/>
            </outSequence>
        </resource>
        <handlers>
            <handler class="org.wso2.carbon.apimgt.gateway.handlers.throttling.APIThrottleHandler">
                <property name="id" value="A"/>
                <property name="policyKey" value="gov:/apimgt/applicationdata/test-tiers.xml"/>
            </handler>
            <handler class="org.wso2.carbon.apimgt.gateway.handlers.ext.APIManagerExtensionHandler"/>
        </handlers>
    </api>
</definitions>
